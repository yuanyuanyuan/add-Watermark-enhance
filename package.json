{
  "name": "watermark-enhancement-product",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "./scripts/test.sh",
    "test:vitest": "vitest",
    "test:unit": "vitest run src/**/*.test.ts",
    "test:integration": "vitest run src/__tests__/integration/",
    "test:e2e": "vitest run src/__tests__/e2e/",
    "test:compatibility": "vitest run src/__tests__/compatibility/",
    "test:performance": "vitest run src/__tests__/performance/",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:coverage:open": "vitest run --coverage && open coverage/index.html",
    "test:p0": "vitest run src/utils/cdn/__tests__/ src/engines/canvas/__tests__/ src/stores/__tests__/",
    "test:p1": "vitest run src/__tests__/integration/ src/__tests__/compatibility/",
    "test:p2": "vitest run src/__tests__/e2e/ src/__tests__/performance/",
    "test:all": "vitest run --coverage --reporter=verbose",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@pdf-lib/fontkit": "^1.1.1",
    "docx": "^9.5.1",
    "docx-preview": "^0.3.6",
    "jszip": "^3.10.1",
    "mammoth": "^1.10.0",
    "pdf-lib": "^1.17.1",
    "pdfjs-dist": "^5.4.54",
    "pizzip": "^3.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "zustand": "^4.4.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^20.19.11",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.0",
    "@vitest/coverage-v8": "^1.0.0",
    "@vitest/ui": "^1.0.0",
    "autoprefixer": "^10.4.0",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.0",
    "jsdom": "^23.0.0",
    "postcss": "^8.4.0",
    "tailwindcss": "^3.3.0",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "vitest": "^1.0.0"
  }
}